<mvc:View xmlns:filterbar="sap.ui.comp.filterbar" controllerName="comep.comep.controller.DemandeList"
    xmlns="sap.ui.table"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:u="sap.ui.unified"
	xmlns:c="sap.ui.core"
	xmlns:m="sap.m"
    xmlns:layout="sap.ui.layout"
    xmlns:f="sap.f">

    <f:DynamicPage showFooter="false" id="ObjectPageLayout" headerExpanded="true" class="sapUiSizeCompact sapUiNoContentPadding">
<f:title>
			<f:DynamicPageTitle id ="DynamicPageTitle" backgroundDesign="Transparent">
				<f:heading>
					<m:Title id="idtitle" text="{i18n>Title}" tooltip="{i18n>Title}"/>
				</f:heading>
				
			</f:DynamicPageTitle>
		</f:title>
        <f:header>
			<f:DynamicPageHeader pinnable="true"  id="idHeader">
				<f:content >
					
				</f:content>
			</f:DynamicPageHeader>
		</f:header>
        <f:content >
        <m:Table id="idTable" sticky="HeaderToolbar,InfoToolbar,ColumnHeaders" growing="true" growingThreshold="10" growingScrollToLoad="true"
					busyIndicatorDelay="30" width="100%" items="{path:'/demandeSet'}" updateFinished="onUpdateFinished"
				    class="sapUiSizeCompact stickyToolbar sapUiNoContentPadding">
					<m:columns class=" sapUiSizeCompact stickyToolbar">
						<m:Column id="demandeID" minScreenWidth="" >
                            <m:Label text="{i18n>demande}" tooltip="{i18n>demande}"/>
						</m:Column>
                        <m:Column id="demDescr" minScreenWidth="" >                         
							<m:Label text="{i18n>description}" tooltip="{i18n>description}"/>                      
						</m:Column>
                       
                        </m:columns>
					<m:items  class=" sapUiSizeCompact">
						<m:ColumnListItem type="Navigation" press="onDemandePress" selected="true" >
							<m:cells>
                            <!-- <m:ObjectIdentifier Id ='ObjectIdentifier' title="{dem_code}"  /> -->
                            <m:Text  text="{dem_descr}"  />
                            <m:Text  text="{dem_code}"  />
															                                
                                </m:cells>
						</m:ColumnListItem>
					</m:items>
				</m:Table>
        </f:content>
    </f:DynamicPage>
</mvc:View>
