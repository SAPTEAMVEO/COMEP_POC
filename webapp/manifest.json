{
  "_version": "1.49.0",
  "sap.app": {
    "id": "comep.comep",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.7",
      "toolsId": "1c14fcf8-6ecd-47c5-b7f7-8e15b5ca275a"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZCOMEP_POC_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.114.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "comep.comep.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "comep.comep.view",
        "controlId": "app",
        "viewLevel": 1
      },
      "routes": [
        {
          "name": "RouteDemandeList",
          "pattern": "",
          "target": [
            "TargetDemandeList"
          ],
          "layout": "OneColumn"
        },
        {
          "pattern": "DemandeDetail/{dem_descr}",
          "name": "DemandeDetail",
          "target": [
            "TargetDemandeList",
            "TargetDemandeDetail"
          ]
        }
      ],
      "targets": {
        "TargetDemandeList": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "DemandeList",
          "viewName": "DemandeList",
          "controlAggregation": "pages"
        },
        "TargetDemandeDetail": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "DetailDemande",
          "viewName": "DetailDemande",
          "controlAggregation": "pages"
        }
      }
    },
    "rootView": {
      "viewName": "comep.comep.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}